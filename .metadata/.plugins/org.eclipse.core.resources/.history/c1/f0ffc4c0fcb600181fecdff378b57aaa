package jp.co.internous.action;

public class  Test{

	public static void main(String[] args) {

		Person3 taro = new Person3();
		taro.name = "taro";
		taro.age = 18;
		System.out.println(taro.name);
		System.out.println(taro.age);

		Person3 jiro = new Person3("jiro", 20);
		System.out.println(jiro.name);
		System.out.println(jiro.age);

		System.out.println("----------------------");

		Person3 saburo = new Person3("saburo",0);
		System.out.println(saburo.name+","+saburo.age);

		Person3 nanashi = new Person3(25);
		System.out.println(nanashi.name+","+nanashi.age);

		Person3 hanako = new Person3("hananko", 17);
		System.out.println(hanako.name+","+hanako.age);
	}

}
