package jp.co.internous.action;

import org.omg.CosNaming.NamingContextExtPackage.AddressHelper;

public class  Test{

	public static void main(String[] args) {
		Person kimura = new Person();
		Person suzuki = new Person();
		Person higashi = new Person();

		kimura.name ="木村次郎";
		kimura.age=18;
		kimura.phoneNumber ="070-0000-0000";
		kimura.address ="abc@co,jp";
		kimura.seibetsu = false; //falseが男、trueは女
		kimura.seinengappi = "1999/12/18";

		suzuki.name ="鈴木花子";
		suzuki.age=16;
		suzuki.phoneNumber = "080-0000-0000";
		suzuki.address = "def@go.com";
		suzuki.seibetsu = true;
		suzuki.seinengappi = "2002/8/15";

		higashi.name ="東恩納ノア";
		higashi.age=23;
		higashi.phoneNumber = "090-0000-0000";
		higashi.address = "123@ya.jp";
		higashi.seibetsu = true;
		higashi.seinengappi = "1994/10/13";

		//演習１
		System.out.println(kimura.name + "、"+ kimura.age);
		System.out.println(suzuki.name + "、"+ suzuki.age);
		System.out.println(higashi.name + "、"+ higashi.age);

	}

}
