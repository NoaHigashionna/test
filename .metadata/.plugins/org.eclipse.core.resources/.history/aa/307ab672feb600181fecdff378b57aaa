		suzuki.name = "鈴木花子";
		suzuki.age = 16;
		higashi.name = "東恩納ノア";
		higashi.age = 20;

		System.out.println(kimura.name);
		System.out.println(kimura.age);
		System.out.println(suzuki.name);
		System.out.println(suzuki.age);
		System.out.println(higashi.name);
		System.out.println(higashi.age);

	}

