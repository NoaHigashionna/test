public class SmartPhone extends Phone implements Mp3PlayerIF, NewFunctionIF{

	public void call(){
		System.out.println("電話(Smartphone)");
	}
	public void mail(){
		System.out.println("メール(Smartphone)");
	}
	public void photo(){
		System.out.println("写真(Smartphone)");
	}
	public void internet(){
		System.out.println("インターネット(Smartphone)");
	}

}
